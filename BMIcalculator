#include <stdio.h>
#include <time.h>
#include <math.h>



int main()
{
    float w;
    float h;
    
    printf("Input your height (cm) and your weight (kg)");
    scanf("%f%f", &h, &w); //scans for your height and weight
    
    
    float BMI = w / pow(h / 100., 2); //counts your BMI
    printf("\n\nYour BMI = %.3f", BMI);
    return 0;
}
