#include <stdio.h>

int main()
{
 int x, y, z;

 printf("Please Enter three different numbers\n");
 scanf("%d %d %d", &x, &y, &z);
 
 if (x > y && x > z){
   printf("\n%d is Highest Among both %d and %d", x, y, z); 
  }
 
 
 else if (y > x && y > z){
   printf("\n%d is Highest Among both %d and %d", y, x, z);
  }
 
 
 else if (z > x && z > y){
   printf("\n%d is Highest Among both %d and %d", z, x, y);
  }
 
 
 else{
   printf("\nEither any two values or all the three values are equal");
  } 
 
printf("\n\n\nThanks for visiting, have a nice day :), bye");
 
 return 0;
}
