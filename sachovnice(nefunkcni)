#include <stdio.h>

int main() 
{
int s = 10;
for (int x = 0; x < s; x++) 
 {
      for (int y = 0; y < s; y++)
   {
       {
        if ((x + y) % 2 == 0)
        {
            printf("O");
        } 
        else
        {
            printf("X"); 
        }    
   } 
  printf("/n");
 }
    return 0;
}
}

