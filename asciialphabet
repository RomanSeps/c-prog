#include <stdio.h>

int main() {
    char ascii = 'A';
    char ascii1 = 'a';

    printf("ASCII HIGH ALPHABET");
    for(int i = 0; i < 26; i++){
        printf("\n0%i = %c", ascii, ascii);
        ascii += 1;

    }

    printf("\n\nASCII LOW ALPHABET");
    for(int j = 0; j < 26; j++){
        if(ascii1 < 100){
            printf("\n0%i = %c", ascii1, ascii1);
            ascii1 += 1;
            continue;
        }else{
        printf("\n%i = %c", ascii1, ascii1);
        ascii1 += 1;
        }
    }
    return 0;
}
