#include <stdio.h>


int main() {
int prvni = 0;
int druhe = 1;
int dalsicislo = prvni + druhe;

printf("Fibonacciho posloupnost: %d, %d, ", prvni, druhe);

for(int i = 1; i <= 10; i++){
    printf("%d, ", dalsicislo);
    prvni = druhe;
    druhe = dalsicislo;
    dalsicislo = prvni + druhe;
}

    return 0;
}
